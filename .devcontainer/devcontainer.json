// https://github.com/microsoft/vscode-dev-containers/tree/v0.222.0/containers/codespaces-linux
{
  "name": "GitHub Codespaces (Default)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot",
        "github.vscode-github-actions",
        "GitHub.vscode-pull-request-github",
        "humao.rest-client"
      ]
    }
  }
}
